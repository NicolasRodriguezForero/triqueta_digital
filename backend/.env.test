# Test environment configuration
APP_NAME=Triqueta Digital API - Tests
APP_VERSION=1.0.0
DEBUG=True
ENVIRONMENT=test

# Database - using separate test database
DATABASE_URL=postgresql+asyncpg://triqueta_user:triqueta_pass@db:5432/triqueta_test

# Redis (not used in tests but required by settings)
REDIS_URL=redis://redis:6379/1

# Security
SECRET_KEY=test-secret-key-change-in-production
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=30

# CORS
BACKEND_CORS_ORIGINS=["http://localhost:5173","http://localhost:3000"]

# Rate Limiting - DISABLED IN TESTS
RATE_LIMIT_ENABLED=False
RATE_LIMIT_LOGIN=5/15minute
RATE_LIMIT_REGISTER=3/hour
